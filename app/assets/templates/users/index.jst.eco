
<% if @users.length: %>
  <table>
    <tr>
      <th>ID</th>
      <th>NAME</th>
      <th>EMAIL</th>
      <th></th>
    </tr>
  <% for user in @users: %>
    <tr>
      <td><%= user.id %></td>
      <td><a href=""><%= user.name %></a></td>
      <td><%= user.email %></td>
      <td><a href="" class="show">Show</a> <a href="" class="edit">Edit</a> <a href="" class="delete">Delete</a></td>
    </tr>
  <% end %>
  </table>
<% else: %>
  No users
<% end %>
